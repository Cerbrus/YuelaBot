<tr id="<%= dom_id(game_key) %>">
  <td class="whitespace-nowrap py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:pl-6"><%= game_key.name %></td>
  <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500"><%= game_key.servers.first.name %></td>
  <td class="relative whitespace-nowrap py-4 pl-3 pr-4 text-right text-sm font-medium sm:pr-6">
    <% if key_visible %>
      <%= tag.td game_key.key, class: "whitespace-nowrap px-3 py-4 text-sm"%>
    <% else %>
      <%= tag.td t(".claim"), class: "cursor-pointer whitespace-nowrap px-3 py-4 text-sm text-gray-500", data: { "claim-button-confirm": t(".confirm-claim"), controller: "claim-button", action: "click->claim-button#claim", game_key: game_key.to_signed_global_id.to_s } %>
    <% end %>
  </td>
</tr>